{
    "extends": "google",
    "env": {
        "es6": true
    },
    "rules": {
        "require-jsdoc": "warn",
        "valid-jsdoc": "warn",
        "no-trailing-spaces": ["error", 
            {
                "skipBlankLines": true
            }
        ],
        "max-len": "warn",
        "no-unused-vars": "warn"
    }
}